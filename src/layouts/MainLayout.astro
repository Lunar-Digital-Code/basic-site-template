---
// component imports
import Head from "../components/Head.astro";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";

export interface Props {
  pageTitle: string;
}

const { pageTitle } = Astro.props;
---

<html lang="en">
  <Head pageTitle={pageTitle} />
  <Navbar />
  <body>
    <slot />
    <Footer />
  </body>
</html>
